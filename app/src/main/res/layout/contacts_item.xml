<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearlayout_child"
    android:layout_width="fill_parent"
    android:layout_height="@dimen/gd_netset_netchoice_h"
    android:background="@drawable/list_chlid_press_sel"
    android:orientation="horizontal" >

    <ImageView
        android:id="@+id/child_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dip" />

    <LinearLayout
        android:id="@+id/linearlayout_child"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/gd_netset_netchoice_h"
        android:background="@drawable/list_chlid_press_sel"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dip"
            android:src="@drawable/pic_contact_list_mall" />

        <TextView
            android:id="@+id/item_text"
            style="@style/gd_netset_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:paddingLeft="20dip"
            android:text="@string/southeast" />

        <ImageView
            android:id="@+id/contats_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/gd_image_jiantou_r"
            android:src="@drawable/arrow" />
    </LinearLayout>

</LinearLayout>